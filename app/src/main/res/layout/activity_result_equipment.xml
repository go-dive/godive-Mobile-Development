<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.equipment.ResultEquipmentActivity">

  <ImageView
      android:id="@+id/img_health"
      android:layout_width="303dp"
      android:layout_height="288dp"
      android:layout_marginTop="200dp"
      android:src="@drawable/icon_test"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/desc_health"
      android:layout_width="310dp"
      android:layout_height="wrap_content"
      android:text="@string/decs_result_equipment"
      android:textColor="@color/black"
      android:textSize="18dp"
      android:layout_marginTop="15dp"
      app:layout_constraintTop_toBottomOf="@id/img_health"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      android:textAlignment="center" />


</androidx.constraintlayout.widget.ConstraintLayout>